{
  "name": "vtex-whoami",
  "displayName": "VTEX Whoami",
  "description": "VTEX Whoami is a VSCode extension that allows you to show VTEX account/workspace informations in the status bar.",
  "icon": "assets/VTEX-WHOAMI.png",
  "version": "0.0.4",
  "publisher": "milaniromulo",
  "author": {
		"name": "RÃ´mulo Milani",
		"email": "milaniromulo@gmail.com"
	},
  "engines": {
    "vscode": "^1.75.0"
  },
  "license": "MIT",
	"homepage": "https://github.com/milaniromulo/vtex-whoami",
	"repository": {
		"type": "git",
		"url": "https://github.com/milaniromulo/vtex-whoami.git"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"statusbar",
		"status",
		"workspace",
		"VTEX",
		"VTEX CLI",
		"VTEX Toolbelt"
	],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vtex-whoami.whoami",
        "category": "VTEX",
        "title": "whoami"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}